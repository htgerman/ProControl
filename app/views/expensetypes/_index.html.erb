<h2>Expense Types</h2>
<p><%= link_to 'New Expense Type', new_expensetype_path %></p>
<table>
  <tr>
    <th>Name</th>
    <th class="descr_list">Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @expensetypes.each do |expensetype| %>
  <tr>
    <td><span class="itemtitle"><%= link_to "#{expensetype.name}", edit_expensetype_path(expensetype) %></span></td>
    <td class="descr_list"><%= expensetype.description %></td>
    <td><%= link_to 'Delete', expensetype, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

